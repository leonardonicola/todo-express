{
  "name": "backend",
  "version": "1.0.0",
  "description": "Servidor Express.js",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --transpile-only src/server.ts",
    "typeorm": "ts-node-dev node_modules/.bin/typeorm",
    "migration:up": "ts-node-dev node_modules/.bin/typeorm migration:run -d src/config/ormconfig.ts",
    "migration:down": "ts-node-dev node_modules/.bin/typeorm migration:revert -d src/config/ormconfig.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/uuid": "^9.0.0",
    "body-parser": "^1.20.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "author": "Leonardo Nicola",
  "license": "MIT"
}
